/* common/leaderlist/leaderlist.wxss */

.main {
    width: 100%;
    height: 1200rpx;
    background-color: rgb(128, 165, 134);
}
.main .uptxt {
    padding-left: 42%;
    font-size: 33rpx;
    display: block;
}
.main .leader {
    width: 33%;
    border: 1px solid #D7F0DB;
    background-color: #fff;
    height: 40px;
    border-radius: 20rpx;
    text-align: center;
    font-size: 40rpx;
    float: left;
    margin-top: 40rpx;
}
.cu-btn {
    margin: 100px auto;
    float: right;
}


.main .form-box {
    margin-top: 200rpx;
    padding-left: 30rpx;
}
.form-box input{
    height: 150rpx;
    border-bottom: 8rpx solid #ffffff;
    color: #fff;
}
.form-box .input-placeholder{
    font-size: 40rpx;
    color: #ffffff;
}
.form-box button{
    margin:30rpx 30rpx 0 0;
}